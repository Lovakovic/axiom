    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.7",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@langchain/anthropic": "^0.3.20",
    "@langchain/core": "^0.3.55",
    "@langchain/google-vertexai": "^0.2.9",
