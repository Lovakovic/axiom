{
  "name": "mcp",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start:server": "npm run build && node dist/server/index.js",
    "start:server:sse": "npm run build && node dist/server/sse.js",
    "start:agent": "npm run build && node dist/cli.js",
    "dev:server": "concurrently \"npm run watch\" \"nodemon --watch dist/server dist/server/index.js\"",
    "dev:server:sse": "concurrently \"npm run watch\" \"nodemon --watch dist/server dist/server/sse.js\"",
    "dev:agent": "concurrently \"npm run watch\" \"nodemon --watch dist dist/cli.js\"",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.7",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.11",
    "@langchain/core": "^0.3.30",
    "@langchain/langgraph": "^0.2.40",
    "@modelcontextprotocol/sdk": "^1.1.1",
    "dotenv": "^16.4.7",
    "mime-types": "^2.1.35",
    "zod": "^3.24.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "packageManager": "yarn@4.6.0"
}
