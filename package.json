    "@langchain/openai": "^0.5.10",
    "@modelcontextprotocol/sdk": "^1.1.1",
    "@types/jest": "^29.5.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "mime-types": "^2.1.35",
    "ts-jest": "^29.2.6",
    "uuid": "^11.1.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "packageManager": "yarn@4.6.0"
